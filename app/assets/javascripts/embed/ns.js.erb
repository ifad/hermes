!(function(w, h){
  'use strict';
  
  h = w.__hermes_embed = {};
  h.assets = {
    logo: "<%= asset_url('hermes-logo.png') %>"
  };
  h.host = "<%= compute_asset_host %>";
  h.env = "<%= Rails.env %>";
  h.hash = document.location.hash;
  document.location.hash = '';
  h.pagepath = w.location.protocol + h.host;

})(this);